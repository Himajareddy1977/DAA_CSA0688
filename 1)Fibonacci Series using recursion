#include<stdio.h>
int fibinacci(int i){
	if(i==0)
		return 0;
	else if(i==1)
		return 1;
	else
		return (fibinacci(i-1)+fibinacci(i-2));
}
int main(){
	int num,i,a=0,b=1;
	printf("enter the number: ");
	scanf("%d",&num);
	printf("the fibinacci series = %d %d ",a,b);
	for(i=2;i<=num;i++){
	printf("%d ",fibinacci(i));
	}
}

